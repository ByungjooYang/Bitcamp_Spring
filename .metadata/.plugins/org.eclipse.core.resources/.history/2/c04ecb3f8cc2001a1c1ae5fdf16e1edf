package sample03;

import lombok.Data;

@Data
public class SungJukDTO {
	private String name;
	private int kor;
	private int eng;
	private int math;
	private int tot;
	private double avg;
	
	@Override
	public String toString() {
		return getName() + "\t" + getKor()  + "\t" +  getEng()  + "\t" + getMath()  + "\t" + getTot()  + "\t" +  getAvg();
	}
}
